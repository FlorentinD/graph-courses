{
  "name": "@neo4j-education/graphacademy",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=dev npm run dev:serve & npm run dev:css & npm run dev:ui & npm run dev:cmd",
    "dev:serve": "nodemon --ignore './resources' --exec ts-node src/main.ts",
    "dev:cmd": "nodemon -e adoc --ignore 'asciidoc/pages/*.adoc' --exec ts-node src/commands/load-content.ts",
    "dev:css": "sass --watch resources/scss/app.scss public/css/app.css",
    "dev:ui": "webpack",
    "dev:img": "ts-node src/commands/generate-images.ts",
    "uglify": "npm run uglify:css && npm run uglify:ui",
    "uglify:css": "uglifycss public/css/app.css > public/css/app.min.css && rm public/css/app.css && mv public/css/app.min.css public/css/app.css",
    "uglify:ui": "uglifyjs public/js/bundle.js > public/js/bundle.min.js && rm public/js/bundle.js && mv public/js/bundle.min.js public/js/bundle.js",
    "build": "tsc && npm run build:css && npm run build:ui",
    "build:css": "sass resources/scss/app.scss public/css/app.css && npm run uglify:css",
    "build:ui": "webpack --config webpack-prod.config.js  && npm run uglify:ui",
    "start": "NODE_ENV=prod node --max-http-header-size=24000 dist/main.js",
    "watch": "nodemon --exec ts-node -e adoc src/commands/load-content.ts",
    "cypress:open": "CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "cypress:prod": "CYPRESS_BASE_URL=https://graphacademy.neo4j.com cypress open",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "og:generate": "ts-node src/commands/generate-images.ts",
    "browser": "open https://8ee9d883.databases.neo4j.io/browser/",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-flash": "^0.0.2",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.2",
    "@types/mailgun-js": "^0.22.12",
    "@types/pug": "^2.0.4",
    "cypress": "^8.4.0",
    "jest": "^27.2.5",
    "node-html-to-image": "^3.2.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3",
    "uglify-js": "^3.14.1",
    "uglifycss": "^0.0.29",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@asciidoctor/core": "^2.2.4",
    "@bugsnag/js": "^7.10.5",
    "@bugsnag/plugin-express": "^7.10.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-openid-connect": "^2.3.1",
    "express-session": "^1.17.2",
    "highlight.js": "^10.7.2",
    "html-entities": "^2.3.2",
    "mailgun-js": "^0.6.7",
    "neo4j-driver": "^4.2.3",
    "nodemon": "^2.0.7",
    "normalize.css": "^8.0.1",
    "pug": "^3.0.2",
    "sass": "^1.32.12",
    "ts-loader": "^9.1.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.37.0"
  },
  "resolutions": {
    "agent-base": "^6.0.1"
  }
}
